<%=  form_for @issue do |f| %>
<div class="field">
	<%= f.label :note %>
	<%= f.text_area :note %>
</div>
<div class="field">
  <%= f.label 'Property' %><br>
  <%= collection_select(:issue, :property_id, Property.all, :id, :address) %>
</div>

<div class="actions">
    <%= f.submit class:"btn btn-success"%>
  </div>

<% end %>